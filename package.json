	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/phillipivan/companion-module-crestron-HD-MDNXM-4KZ-E.git"
	},
	"engines": {
		"node": "^22.20",
		"yarn": "^4"
	},
	"dependencies": {
		"@companion-module/base": "~1.14.1",
		"@traversable/zod": "^0.0.57",
		"axios": "^1.13.2",
		"axios-cookiejar-support": "^6.0.5",
		"es-toolkit": "^1.43.0",
		"p-queue": "^9.1.0",
		"tough-cookie": "^6.0.0",
		"ws": "^8.19.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@companion-module/tools": "^2.5.0",
		"@types/node": "^22.19.6",
		"@types/ws": "^8.18.1",
		"eslint": "^9.39.2",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"rimraf": "^6.1.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.53.0"
	},
	"prettier": "@companion-module/tools/.prettierrc.json",
	"lint-staged": {
		"*.{css,json,md,scss}": [
			"prettier --write"
		],
		"*.{ts,tsx,js,jsx}": [
			"yarn lint:raw --fix"
		]
	},
	"packageManager": "yarn@4.12.0"
}
